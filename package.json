{
  "name": "apitemplate",
  "version": "1.0.0",
  "description": "Express Typescript Template with Rest API",
  "main": "./bin/www",
  "scripts": {
    "start": "npm run build && node ./bin/www",
    "dev": "nodemon --ext ts --watch src --exec './node_modules/.bin/ts-node' src/bin/www.ts",
    "build": "node ./bin/build",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/mocha --exit"
  },
  "keywords": [
    "typescript",
    "express",
    "nodejs"
  ],
  "author": "Soe Minn Minn",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/formidable": "^1.0.31",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/knex": "^0.15.2",
    "@types/mysql": "^2.15.6",
    "@types/node": "^11.13.18",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/shelljs": "^0.8.5",
    "@types/uuid": "^3.4.5",
    "mocha": "^6.2.0",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "formidable": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "uuid": "^3.3.2"
  }
}
